{
  "env": {
      "browser": true,
      "es2020": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
        "_id"
        ]
      }
    ]
  }
}
